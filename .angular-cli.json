{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "nationwide-project"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
        
      ],
      "scripts": [
         
        
         "ui-framework-1.1.2/js/jquery.js",
         "ui-framework-1.1.2/js/bootstrap.js",
         "ui-framework-1.1.2/js/moment.js",
         "ui-framework-1.1.2/js/bootstrap-datetimepicker.js",
         "ui-framework-1.1.2/js/jquery.tablesorter.js",
         "ui-framework-1.1.2/js/jquery-spin.js",
         "ui-framework-1.1.2/js/bootstrap-multiselect.js",
         "ui-framework-1.1.2/js/typeahead.bundle.js",
         "ui-framework-1.1.2/js/jquery-mask.js",
         "ui-framework-1.1.2/js/bootstrap-custom.js"

      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
